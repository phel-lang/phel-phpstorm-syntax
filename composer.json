{
    "name": "phel-lang/phel-phpstorm-syntax",
    "keywords": ["phel","lisp","functional","language"],
    "homepage": "https://phel-lang.org/",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "phel-lang/phel-lang": "^0.9",
        "gacela-project/gacela": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "PhelPhpStorm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhelPhpStormTests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "update-readme": [
            "php update-readme.php"
        ],
        "test": "./vendor/bin/phpunit"
    }
}
